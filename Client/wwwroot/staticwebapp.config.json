{
  "navigationFallback": {
    "rewrite": "index.html",
    "exclude": ["/_framework/*", "/css/*", "/.auth/*", "/login"]
  },  
  "routes": [

      {
        "route":"/login",
        "redirect":"/.auth/login/aad"
      },
      {
        "route": "/",
        "rewrite": "index.html",
        "allowedRoles": ["anonymous","autehnticated"]
      },
      {
        "route":"/currentweather",
        "allowedRoles":["authenticated"]
      },
      {
        "route": "/.auth/login/twitter",
        "statusCode": "404"
      }
    ],
   "responseOverrides": {
    "400": {
      "rewrite": "/invalid-invitation-error.html"
    },
    "401": {
      "redirect": "/login",
      "statusCode": 302
    },
    "403": {
      "rewrite": "/custom-forbidden-page.html"
    },
    "404": {
      "rewrite": "/404.html"
    }
  },
  }
